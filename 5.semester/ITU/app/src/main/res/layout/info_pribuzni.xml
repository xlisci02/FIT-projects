<?xml version="1.0" encoding="utf-8"?>
<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
             android:orientation="vertical"
             android:layout_width="match_parent"
             android:layout_height="wrap_content"
             android:stretchColumns="*"
             android:id="@+id/expandTable1"
>


	<TableRow>
		<TextView
				android:textSize="16dip"
				android:textStyle="bold"
				android:paddingLeft="65dp"
				android:paddingTop="5dp"
				android:paddingBottom="5dp"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:text="Užívateľské meno:"/>

		<TextView
				android:id="@+id/lblListItem11"
				android:textSize="16dip"
				android:paddingLeft="15dp"
				android:paddingTop="5dp"
				android:paddingBottom="5dp"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:text=" "/>
	</TableRow>

	<TableRow>

		<TextView
				android:textSize="16dip"
				android:textStyle="bold"
				android:paddingLeft="65dp"
				android:paddingTop="5dp"
				android:paddingBottom="5dp"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:text="Vek:"/>

		<TextView
				android:id="@+id/lblListItem21"
				android:textSize="16dip"
				android:paddingLeft="15dp"
				android:paddingTop="5dp"
				android:paddingBottom="5dp"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:text=" "
		/>
	</TableRow>


	<TableRow android:layout_marginTop="10dp">
		<Button
				android:textColor="#fff"
				android:background="@drawable/mybuttonstyle"
				android:layout_marginTop="0dp"
				android:layout_gravity="center_horizontal"
				android:layout_height="37dp"
				android:layout_width="wrap_content"
				android:layout_marginBottom="10dp"

				android:text="VYZVAŤ V MINIHRE"
			android:layout_marginLeft="30dp"
			android:layout_marginRight="-30dp"/>
		<Button
				android:textColor="#fff"
				android:background="@drawable/del_button_style"
				android:layout_marginTop="0dp"
				android:layout_gravity="center_horizontal"
				android:layout_width="wrap_content"
				android:layout_height="37dp"
				android:layout_marginBottom="15dp"
				android:text="odstrániť" android:layout_marginRight="40dp"/>
	</TableRow>

</TableLayout>